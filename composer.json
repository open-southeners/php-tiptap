{
    "name": "open-southeners/tiptap",
    "description": "Extract or analyse content from Tiptap generated JSON using PHP",
    "license": "MIT",
    "keywords": [
        "open-southeners"
    ],
    "authors": [
        {
            "name": "Ruben Robles",
            "email": "d8vjork@outlook.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "open-southeners/laravel-helpers": "^0.9"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "laravel/pint": "^1.0",
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "OpenSoutheners\\Tiptap\\": "src",
            "OpenSoutheners\\Tiptap\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}